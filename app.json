{
  "expo": {
    "name": "HelloWorld",
    "slug": "spotify-clone",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "height": 70,
      "backgroundColor": "#121212"
    },
    "androidStatusBarColor": "#fff",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "rgba(0,0,0,0.5)"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
